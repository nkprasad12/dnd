{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
    "require-jsdoc" : 0,
    "valid-jsdoc" : 0,
    "ignoreUrls": 0,
    "max-len": [1, 80, 2, {
      "ignorePattern": "^import\\s.+\\sfrom\\s.+;$",
      "ignoreUrls": true
    }]
  }
}
